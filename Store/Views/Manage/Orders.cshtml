@model IEnumerable<StoreModel.Models.Order>




<table>

    <tr>
      
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tr>

    @foreach (var order in Model)
    {

        <tr>
            
            <td>@order.Product.Title.ToString()</td>
            <td>@order.OrderDate</td>
            <td>@order.Count</td>
            <td>@order.Product.Price</td>
            <td>@order.Cost</td>
        </tr>

    }

</table>

<div>
    @ViewBag.TotalCost
</div>



